{{#collection tagName="ul" itemViewClass="Ember.View" itemClass="repository" contentBinding="serviceHooks"}}
  <a {{bindAttr href="content.url"}} rel="nofollow">{{content.name}}</a>
  <p class="description">{{content.description}}</p>
  <div class="controls">
    <a {{bindAttr href="content.urlGithubAdmin"}} class="github-admin tool-tip" title="Github service hooks admin page"></a>
    <a {{bindAttr class=":switch content.active:active"}} {{action "toggle" target="content"}}</a>
  </div>
{{/collection}}
