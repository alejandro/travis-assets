<table id="branch_summary">
  <thead>
    <tr>
      <th>{{t "repositories.branch"}}</th>
      <th>{{t "repositories.commit"}}</th>
      <th>{{t "repositories.message"}}</th>
      <th>{{t "repositories.started_at"}}</th>
      <th>{{t "repositories.finished_at"}}</th>
    </tr>
  </thead>
  {{#collection tagName="tbody" contentBinding="branches" itemClassBinding="content.color"}}
      <td class="number"><a {{bindAttr href="view.content.buildUrl"}}>{{view.content.branch}}</a></td>
      <td class="commit"><a {{bindAttr href="view.content.commitUrl"}}>{{view.content.formattedCommit}}</a></td>
      <td class="message">{{{view.content.formattedShortMessage}}}</td>
      <td class="duration" {{bindAttr title="view.content.started_at"}}>{{view.content.started_at}}</td>
      <td class="finished_at timeago" {{bindAttr title="view.content.finished_at"}}>{{view.content.finished_at}}</td>
  {{/collection}}
</table>
